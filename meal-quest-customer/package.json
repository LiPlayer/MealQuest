{
  "name": "meal-quest-customer",
  "version": "1.0.0",
  "private": true,
  "description": "MealQuest Customer App",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "prepare": "husky",
    "new": "taro new",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src test config",
    "typecheck": "tsc --noEmit",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "test": "jest --config jest.config.js",
    "test:regression:ui": "jest test/pages/startup.test.tsx test/pages/account.test.tsx --runInBand",
    "test:e2e": "jest --config jest.e2e.config.js --runInBand",
    "test:e2e:core": "jest --config jest.e2e.config.js --runInBand test/e2e/customer-core-flow.spec.js",
    "test:e2e:weapp": "npm run build:weapp && npm run test:e2e",
    "test:e2e:doctor": "node ./test/e2e/doctor.js",
    "test:watch": "jest --watch"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@tarojs/components": "4.1.11",
    "@tarojs/helper": "4.1.11",
    "@tarojs/plugin-framework-react": "4.1.11",
    "@tarojs/plugin-platform-alipay": "4.1.11",
    "@tarojs/plugin-platform-h5": "4.1.11",
    "@tarojs/plugin-platform-harmony-hybrid": "4.1.11",
    "@tarojs/plugin-platform-jd": "4.1.11",
    "@tarojs/plugin-platform-qq": "4.1.11",
    "@tarojs/plugin-platform-swan": "4.1.11",
    "@tarojs/plugin-platform-tt": "4.1.11",
    "@tarojs/plugin-platform-weapp": "4.1.11",
    "@tarojs/react": "4.1.11",
    "@tarojs/runtime": "4.1.11",
    "@tarojs/shared": "4.1.11",
    "@tarojs/taro": "4.1.11",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "4.1.11",
    "@tarojs/plugin-generator": "4.1.11",
    "@tarojs/taro-loader": "4.1.11",
    "@tarojs/webpack5-runner": "4.1.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/minimatch": "^5",
    "@types/node": "^18",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^10.4.24",
    "babel-jest": "^30.2.0",
    "babel-preset-taro": "4.1.11",
    "eslint": "^8.57.0",
    "eslint-config-taro": "4.1.11",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.1.2",
    "miniprogram-automator": "^0.12.1",
    "postcss": "^8.5.6",
    "react-refresh": "^0.14.0",
    "sass": "^1.75.0",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^3.4.19",
    "taro-plugin-tailwind": "^1.3.4",
    "ts-jest": "^29.4.6",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "5.91.0"
  }
}
