# Runtime
NODE_ENV=staging
PORT=3030

# Storage (PostgreSQL)
MQ_DB_URL=postgres://postgres:postgres@127.0.0.1:5432/mealquest
MQ_DB_SCHEMA=public
MQ_DB_SNAPSHOT_KEY=main
MQ_DB_POOL_MAX=5
MQ_DB_AUTO_CREATE=true
MQ_DB_ENFORCE_RLS=true
MQ_DB_ADMIN_URL=

# Security
MQ_JWT_SECRET=staging-jwt-secret-change-me
MQ_PAYMENT_CALLBACK_SECRET=staging-callback-secret-change-me
MQ_ONBOARD_SECRET=

# Social auth providers
MQ_AUTH_WECHAT_MINI_APP_ID=
MQ_AUTH_WECHAT_MINI_APP_SECRET=
MQ_AUTH_ALIPAY_VERIFY_URL=
MQ_AUTH_ALIPAY_APP_ID=
MQ_AUTH_ALIPAY_APP_SECRET=
MQ_AUTH_HTTP_TIMEOUT_MS=10000

# AI strategy planner
MQ_AI_PROVIDER=deepseek
# API keys are injected via secret manager / runtime environment.
MQ_POLICY_TEMPLATE_VALIDATE_ON_BOOT=true
