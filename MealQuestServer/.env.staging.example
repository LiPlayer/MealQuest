# Runtime
NODE_ENV=staging
PORT=3030
LOG_LEVEL=info

# Storage (PostgreSQL)
MQ_DB_URL=postgres://postgres:postgres@127.0.0.1:5432/mealquest
MQ_DB_SCHEMA=public
MQ_DB_STATE_TABLE=mealquest_state_snapshots
MQ_DB_SNAPSHOT_KEY=main
MQ_DB_POOL_MAX=5
MQ_DB_AUTO_CREATE=true
MQ_DB_ADMIN_URL=

# Security
MQ_JWT_SECRET=staging-jwt-secret-change-me
MQ_PAYMENT_CALLBACK_SECRET=staging-callback-secret-change-me

# Payment provider strategy
# Keep mock until real supplier API is available
MQ_PAYMENT_PROVIDER=mock
MQ_PAYMENT_PROVIDER_BASE_URL=
MQ_PAYMENT_PROVIDER_MERCHANT_ID=
MQ_PAYMENT_PROVIDER_API_KEY=

# Invoice provider strategy
MQ_INVOICE_PROVIDER=mock
MQ_INVOICE_PROVIDER_BASE_URL=
MQ_INVOICE_PROVIDER_API_KEY=

# Social auth providers
MQ_AUTH_WECHAT_MINI_APP_ID=
MQ_AUTH_WECHAT_MINI_APP_SECRET=
MQ_AUTH_HTTP_TIMEOUT_MS=10000

# AI strategy planner
MQ_AI_PROVIDER=openai_compatible
MQ_AI_BASE_URL=http://127.0.0.1:11434/v1
MQ_AI_MODEL=qwen2.5:7b-instruct
MQ_AI_API_KEY=
MQ_AI_TIMEOUT_MS=15000
